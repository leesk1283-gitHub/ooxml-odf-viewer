{
  "manifest_version": 3,
  "name": "OOXML/ODF Viewer & Editor",
  "version": "1.0.0",
  "description": "View and edit the internal XML structure of OOXML and ODF files.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}